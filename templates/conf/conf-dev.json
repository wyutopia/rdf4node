{
  "app": {
    "name": "rdf4node",
    "alias": "rdf4node",
    "registry": "rdf4node",
    "tags": [
      "Rapid",
      "Development",
      "Framework",
      "Linux",
      "Alpine",
      "node",
      "amd64"
    ],
    "port": 3000
  },
  "security": {
    "allowOrigin": "*",
    "enableRateLimit": false,
    "enableAuthorization": false
  },
  "httpMonitor": true,
  "dataSources": {
    "default": {
      "type": "mongo",
      "config": {
        "ip": "127.0.0.1",
        "port": 27017,
        "user": "dbo",
        "pwd": "Dev123456",
        "db": "test",
        "authSource": "test"
      },
      "enabled": true
    }
  },
  "redis": {
    "shareConnection": false,
    "shareDatabase": false,
    "servers": {
      "default": {
        "host": "127.0.0.1",
        "port": 6379,
        "password": "Dev#2023"
      }
    }
  },
  "caches": {
    "User": {
      "enabled": true,
      "spec": {
        "engine": "redis",
        "database": 0
      }
    }
  },
  "eventBus": {
    "lo": true,
    "disableEvents": [],
    "persistent": true,
    "engine": "rabbitmq",
    "rabbitmq": {
      "vhost": "rdf4node",
      "connection": {
        "slashes": true,
        "protocol": "amqp",
        "hostname": "127.0.0.1",
        "user": "admin",
        "password": "Dev#2023",
        "port": 5672,
        "options": {
          "heartbeat": 4
        }
      },
      "channels": {
        "default": {
          "exchanges": {
            "global.fanout": {
              "type": "fanout"
            },
            "global.topic": {
              "type": "topic"
            }
          },
          "queues": ["events_q"],
          "bindings": {
            "broadcast": {
              "source": "global.fanout",
              "destination": "events_q",
              "destinationType": "queue",
              "bindingKeys": ["#"]
            }
          },
          "publications": {},
          "subscriptions": {}
        },
        "other": {
          "exchange": {},
          "queues": [],
          "bindings": {},
          "publications": {},
          "subscriptions": {}
        }
      }
    }
  },
  "upload": {
    "engine": "native",
    "hostPath": "uploads/",
    "alioss": {
      "config": {
        "accessKeyId": "<accessKeyId>",
        "accessKeySecret": "<accessKeySecret",
        "bucket": "<bucket-name>",
        "endpoint": "oss-cn-hangzhou.aliyuncs.com"
      },
      "params": {
        
      }
    },
    "minio": {}
  },
  "modules": {
    "UserCtl": {
      "engine": "rabbitmq",
      "channel": "User",
      "pubKey": "pubUserEvents"
    }
  }
}